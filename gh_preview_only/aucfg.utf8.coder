QuotesRE:
;==========================================================================================================
;Flags  Pattern                        \BackRef=(FontStyle,ColorText,ColorBk) \BackRef=...  ParentID RuleID
;==========================================================================================================
;0	`(")([^"\\]*(\\.[^"\\]*)*)(")` `\1=(0,${DEL1},0) \2=(3,${STR},0) \4=(0,${DEL1},0)`  0        0
;--директивы (родительское)--
0	`Pragma \w+`    `\0=(0,0,0)`	0	10
;--язык в директиве AutorunLanguage--
0	`(?<=Pragma AutorunLanguage )[a-z]{2,3}`    `\0=(0,${ATTR},0)`
;--пути без кавычек в Include, LoadLibrary, LoadFont (родительское)--
0	`((?<=Pragma Include )|(?<=LoadLibrary )|(?<=LoadLibrary /U )|(?<=LoadFont )|(?<=LoadFont /N ))[^"'\n\x20]++[\x20\t]*$`    `\0=(0,0,0)`	0	15
;--параметры в командах--
0	`(/[CH]:)([a-z0-9]+)`         `\1=(0,${ATTR},0) \2=(0,${DEL1},0)`
0	`(/K:)(X[12])`                `\1=(0,${ATTR},0) \2=(0,${DEL1},0)`
0	`(/X[12]:?)([a-z]*)`          `\1=(0,${ATTR},0) \2=(0,${DEL1},0)`
0	`(/[a-z_]+[\-:]?)([a-z]*)`    `\1=(0,${ATTR},0) \2=(0,${DEL1},0)`
;  минусовые значения в командах
0	`(?<=/[XYWH]:)-(?=\d)`    `\0=(0,${NUM},0)`
;--команды Total Commander--
1	`cm_[A-Za-z0-9_]+`    `\0=(3,${OP},0)`
;--команды Total Commander пользовательские--
1	`em_[A-Za-z0-9_]+`    `\0=(3,${TAG},0)`
;--указатель после имени типа в параметрах DllCall (может встретиться только в кавычках)--
0 `((?<=char)|(?<=byte)|(?<=uchar)|(?<=boolean)|(?<=short)|(?<=ushort)|(?<=wchar)|(?<=word)|(?<=int)|(?<=long)|(?<=bool)|(?<=hresult)|(?<=uint)|(?<=ulong)|(?<=dword)|(?<=int64)|(?<=uint64)|(?<=ptr)|(?<=hwnd)|(?<=handle)|(?<=int_ptr)|(?<=long_ptr)|(?<=lparam)|(?<=lresult)|(?<=uint_ptr)|(?<=ulong_ptr)|(?<=dword_ptr)|(?<=wparam))\*`    `\0=(2,${OP},0)`	20
;--указатель после str/wstr в параметрах DllCall не поддерживается--
0	`((?<=str)|(?<=wstr))\*`    `\0=(2,#FFFFFF,#FF0000)`	20
;--обозначение переменных в заковыченных блоках--
;  переменные полностью пока не хочется подсвечивать
;  это правило блокирует правило переменных в Quotes
0	`%`	`\0=(4,0,0)`	20
;--Func: открытый список параметров--
0	`(?<=\()\x2E\x2E\x2E(?=\))`    `\0=(2,${ATTR},0)`
;--Func: имена и параметры (родительское)--
0	`(?<=Func )\w+\([\w,\x20]*\)`    `\0=(0,0,0)`	0	14
;        подсветка
0	`\w+`    `\0=(0,${OP},0)`	14
;--ф-ции: изменение переменной в параметре (DllCall)--
0	`(@)(\w+)`    `\1=(2,${VAR},0) \2=(0,${VAR},0)`
;--перенос строк--
0	`(?m)\x20(_)\x20*$`    `\1=(2,${OP},0)`
;--запрет комментария после переноса строк--
0	`(?m)\x20(_)\x20*(#.*)$`    `\1=(0,#FFFFFF,#FF0000) \2=(3,#FF0000,0)`
;--десятичный разделитель в числах--
1	`(?<=\d)[\.\,](?=\d)`    `\0=(0,${NUM},0)`
;--шестнадцатеричные числа--
1	`0x[0-9A-Fa-f]{2,8}`    `\0=(0,${NUM},0)`
;--встроенные константы и макросы--
0 `%?AUTORUN_(CFG|ISADMIN|OSARCH|PATH|RESTARTCOUNT|SUBPROCESS|TCARCH|TCCMDLINE|TCHANDLE|TCLANG|TCLANGFILE|TCPID|THREADID|UTCDIFF|WINDOW)%?`    `\0=(0,${TYPE},0)`	0	11
0 `(?<=\W)%?(ERROR|EXTENDED|SYSERROR|SYSERRORTEXT)%?(?=\W)`    `\0=(2,${VAR},0)`	0	11
;--константы и макросы плагинов--
0 `%?PROCESS_(USERISADMIN|RUNASADMIN|ISELEVATED|INTEGRITYLEVEL)%?`    `\0=(0,${TYPE},0)`	0	11
0 `%?SYSINFO_(OSARCH|OSVERSION|OSSERVICEPACK|DESKTOPWIDTH|DESKTOPHEIGHT|DESKTOPDEPTH|DESKTOPDPI|USERLANG)%?`    `\0=(0,${TYPE},0)`	0	11
0 `%?(LAST_HINT_WINDOW|ACTIVE_WINDOW)%?`    `\0=(0,${TYPE},0)`	0	11
;--стандартные переменные Total Commander--
0 `%?COMMANDER_(DRIVE|EXE|INI_PATH|INI|PATH|INSTANCE|SESSIONNAME|OPEN)%?`    `\0=(0,${VAR},0)`	0	11
;--объекты--
0 `(?<!\\)(?<!\w)(\w++)(\.)(?=add|dir|end|ext|get|has|pop|pos|ptr|set)`   `\1=(2,${AREA},0) \2=(1,${DEL1},0)`
0 `(?<!\\)(?<!\w)(\w++)(\.)(?=\w\w\w\w)`   `\1=(2,${AREA},0) \2=(1,${DEL1},0)`
;--методы объектов (родительское)--
0	`(?<=\w\.)[a-z_]+\x20*(?=\x28)`   `\0=(0,0,0)`	0	13
;--свойства объектов (родительское)--
0	`(?<=\w\.)[a-z_]+`   `\0=(0,0,0)`	0	12
;--сигнализация на забытый знак процента во встроенных переменных и макросах--
0 `(%)AUTORUN_(CFG|ISADMIN|OSARCH|PATH|RESTARTCOUNT|SUBPROCESS|TCARCH|TCCMDLINE|TCHANDLE|TCLANG|TCLANGFILE|TCPID|THREADID|UTCDIFF|WINDOW)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)AUTORUN_(CFG|ISADMIN|OSARCH|PATH|RESTARTCOUNT|SUBPROCESS|TCARCH|TCCMDLINE|TCHANDLE|TCLANG|TCLANGFILE|TCPID|THREADID|UTCDIFF|WINDOW)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
0 `(%)PROCESS_(USERISADMIN|RUNASADMIN|ISELEVATED|INTEGRITYLEVEL)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)PROCESS_(USERISADMIN|RUNASADMIN|ISELEVATED|INTEGRITYLEVEL)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
0 `(%)SYSINFO_(OSARCH|OSVERSION|OSSERVICEPACK|DESKTOPWIDTH|DESKTOPHEIGHT|DESKTOPDEPTH|DESKTOPDPI|USERLANG)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)SYSINFO_(OSARCH|OSVERSION|OSSERVICEPACK|DESKTOPWIDTH|DESKTOPHEIGHT|DESKTOPDEPTH|DESKTOPDPI|USERLANG)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
0 `(%)(ERROR|EXTENDED|SYSERROR|SYSERRORTEXT)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)(ERROR|EXTENDED|SYSERROR|SYSERRORTEXT)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
0 `(%)(LAST_HINT_WINDOW|ACTIVE_WINDOW)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)(LAST_HINT_WINDOW|ACTIVE_WINDOW)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
;--то же для переменных Total Commander--
0 `(%)COMMANDER_(DRIVE|EXE|INI_PATH|INI|PATH|INSTANCE|SESSIONNAME|OPEN)(?!%)`     `\1=(2,#FFFFFF,#FF0000)`	11
0 `(?<!%)COMMANDER_(DRIVE|EXE|INI_PATH|INI|PATH|INSTANCE|SESSIONNAME|OPEN)(%)`    `\2=(2,#FFFFFF,#FF0000)`	11
;--названия диалогов в плагине AddDialog--
0	`DIALOG_COPYMOVE(_(POST|PINNED))?`    `\0=(0,${OP},0)`
;--родительское для Include, LoadLibrary, LoadFont (см. выше)--
;  переменные
0	`(?<!\\)%\w++%`    `\0=(3,${VAR},0)`	15
;  всё остальное
0	`[\w\\\.\-]+`    `\0=(3,${STR},0)`	15
;--подсветка пользовательских функций (моя самодеятельность)
;0	"\b(fn)(_[a-z0-9_]++)(?=\s*\()"	"\1=(3,#790079,0) \2=(0,#790079,0)"	0	0


